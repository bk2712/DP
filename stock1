class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n= prices.size();
        int mini= prices[0],ans= 0;
        for(int i=1;i<n;i++){
            int sell= prices[i];
            int profit= sell-mini;
            mini= min(prices[i],mini);
            ans= max(ans,profit);
        }
        return ans;
    }
};
